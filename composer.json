{
	"name": "alarox-framework/AlaroxFramework",
	"description": "Framework to communicate with REST servers.",
	"type": "library",
	"homepage": "https://github.com/Al4r1c/AlaroxFramework",
	"authors": [
		{
			"name": "KURDIJAKA Y.",
			"email": "yann.kurdijaka@wanadoo.fr"
